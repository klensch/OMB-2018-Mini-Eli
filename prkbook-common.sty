%scale, longueur totale, largeur totale, largeur parterre
\def\parterrefleuri[#1,#2,#3,#4]{
    \begin{tikzpicture}[scale=#1]
        %setup
        \pgfmathsetmacro{\L}{#2} %longueur totale
        \pgfmathsetmacro{\l}{#3} %largeur totale
        \pgfmathsetmacro{\x}{#4} %largeur parterre
        %outer rectangle
        \tkzDefPoint(0,0){A}
        \tkzDefPoint(\L,0){B}
        \tkzDefPoint(\L,\l){C}
        \tkzDefPoint(0,\l){D}
        %inner rectangle
        \tkzDefPoint(0+\x,0+\x){E}
        \tkzDefPoint(\L-\x,\x){F}
        \tkzDefPoint(\L-\x,\l-\x){G}
        \tkzDefPoint(\x,\l-\x){H}
        %dots
        \tkzDefPoint(0,\x){I}
        \tkzDefPoint(\x,0){J}
        \tkzDefPoint(\L-\x,0){K}
        \tkzDefPoint(\L,\x){L}
        \tkzDefPoint(\L,\l-\x){M}
        \tkzDefPoint(\L-\x,\l){N}
        \tkzDefPoint(\x,\l){O}
        \tkzDefPoint(0,\l-\x){P}
        %draw
        \draw[thick,draw=black,fill=mygreen!20] (A) rectangle (C);
        \draw[thick,draw=black,fill=myorange!20] (E) rectangle (G);
        %node
        \draw[thick,dashed] (I) -- node[above]{$\x$}(E) -- node[right]{$\x$}(J);
        \draw[thick,dashed] (K) -- node[left]{$\x$}(F) -- node[above]{$\x$}(L);
        \draw[thick,dashed] (M) -- node[below]{$\x$}(G) -- node[left]{$\x$}(N);
        \draw[thick,dashed] (O) -- node[right]{$\x$}(H) -- node[below]{$\x$}(P);
        \node[below] at (\L/2,\x){$L$};
        \node[right] at (\L-\x,\l/2){$l$};
        \node[] at (\L/2,\l-\x/2){parterre fleuri};
        \tkzDrawPoints[fill=black](A,...,P)
    \end{tikzpicture}
}

%scale, longueur, largeur, position M
